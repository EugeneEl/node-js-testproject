{
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "camelcase": 0,
        "no-trailing-spaces": 0,
        "object-curly-spacing": 0,
        "guard-for-in": 0,
        "no-unused-vars": ["error",
            { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }
        ],
        "quote-props": ["error", "as-needed"]
    }
}